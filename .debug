<?xml version="1.0" encoding="UTF-8"?> 
<ExtensionList>
  <Extension Id="com.example.helloworld.panel">
    <HostList>
      <!-- Comment Host tags according to the apps you want your panel to support -->
      <!-- Photoshop -->
      <Host Name="PHXS" Port="8088"/>
      <!-- InDesign -->
      <Host Name="IDSN" Port="8087" />
      <!-- InCopy -->
      <Host Name="AICY" Port="8086" />
      <!-- Illustrator -->
      <Host Name="ILST" Port="8085"/>
      <!-- Premiere -->
      <Host Name="PPRO" Port="8084" />
      <!-- AfterEffects -->
      <Host Name="AEFT" Port="8083" />
      <!-- Prelude -->
      <Host Name="PRLD" Port="8082" />
      <!-- FLASH Pro -->
      <Host Name="FLPR" Port="8081" />
      <!-- Dreamweaver -->
      <Host Name="DRWV" Port="8080" />
    </HostList>
  </Extension>
</ExtensionList>